{
  "name": "react-redux-boilerplate-extract-text-css-universal",
  "version": "1.0.0",
  "description": "react universal template with redux",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:server:dev": "cross-env NODE_ENV=development NODE_SIDE=server webpack --config './config/webpack.server.babel.js' --colors --display-error-details",
    "build:server": "npm run clean && cross-env NODE_ENV=production NODE_SIDE=server webpack --config './config/webpack.server.babel.js' --colors --display-error-details",
    "build:webpack": "cross-env NODE_ENV=production NODE_SIDE=client webpack -p --config ./config/webpack.client.babel.js --progress",
    "build": "npm run build:server && npm run build:webpack && cross-env NODE_ENV=production API_BASE=http://localhost:3000/api npm run server",
    "heroku": "npm run server",
    "heroku-postbuild": "npm run clean && cross-env NODE_SIDE=server webpack --config './config/webpack.server.babel.js' --colors --progress && cross-env NODE_SIDE=client webpack -p --config ./config/webpack.client.babel.js --colors --progress",
    "start": "npm run dev",
    "dev": "npm run clean && cross-env NODE_ENV=development NODE_SIDE=client babel-node server/start-server-dev.js --presets=es2015,stage-2,react",
    "prod": "npm run build",
    "staging": "cross-env NODE_ENV=staging npm run server",
    "server": "cross-env NODE_SIDE=client babel-node server/start-server.js --presets=es2015,stage-2,react"
  },
  "author": "Jonathan Gertig <jcgertig@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.0",
    "classnames": "^2.2.5",
    "color-it": "^1.2.7",
    "compression": "^1.6.2",
    "core-decorators": "^0.15.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.6",
    "debug": "^2.6.3",
    "ejs": "^2.5.6",
    "emojic": "^1.1.7",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "fetchum": "^3.3.1",
    "fetchum-redux": "^1.2.2",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "git://github.com/jcgertig/html-webpack-template",
    "html-webpack-template-react": "^1.0.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0-beta.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    "universal-webpack": "^0.3.7",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eventsource-polyfill": "^0.9.6",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "webpack-dev-server": "^2.2.1",
    "webpack-hot-middleware": "^2.16.1"
  }
}
