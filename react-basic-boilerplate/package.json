{
  "name": "react-basic-boilerplate",
  "version": "1.0.0",
  "description": "basic react template",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "build": "npm run clean && npm run build:webpack && cross-env NODE_ENV=production npm run server",
    "heroku": "npm run server",
    "heroku-postbuild": "webpack -p --config ./config/webpack.config.js --progress",
    "start": "cross-env NODE_ENV=development npm run server",
    "dev": "npm run start",
    "prod": "npm run build",
    "staging": "cross-env NODE_ENV=staging npm run server",
    "server": "node server.js"
  },
  "author": "Jonathan Gertig <jcgertig@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "8.0.0",
    "babel-core": "^6.22.1",
    "babel-loader": "7.1.3",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "classnames": "^2.2.5",
    "color-it": "^1.2.7",
    "compression": "^1.6.2",
    "cross-env": "5.1.3",
    "css-loader": "0.28.10",
    "emojic": "^1.1.7",
    "express": "^4.14.1",
    "fetchum": "5.1.4",
    "file-loader": "1.1.10",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "git://github.com/jcgertig/html-webpack-template",
    "html-webpack-template-react": "git://github.com/jcgertig/html-webpack-template",
    "json-loader": "^0.5.4",
    "postcss-loader": "2.1.1",
    "precss": "3.1.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0",
    "react-router-dom": "^4.0.0-beta.3",
    "style-loader": "0.20.2",
    "url-loader": "0.6.2",
    "webpack": "3",
    "webpack-merge": "4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "3.0.2",
    "webpack-dev-server": "2",
    "webpack-hot-middleware": "^2.16.1"
  }
}
